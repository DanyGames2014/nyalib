{
  "schemaVersion": 1,
  "id": "nyalib-fluid",
  "version": "${version}",

  "name": "NyaLib Fluid",
  "description": "A module providing a Fluid API",
  "authors": [
    "DanyGames2014"
  ],

  "license": "MIT",
  "icon": "assets/nyalib-fluid/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.danygames2014.nyalib.NyaLibFluid",
      "net.danygames2014.nyalib.init.fluid.ItemListener",
      "net.danygames2014.nyalib.init.fluid.FluidListener",
      "net.danygames2014.nyalib.init.fluid.PacketListener",
      "net.danygames2014.nyalib.init.fluid.TranslationListener",
      "net.danygames2014.nyalib.init.fluid.BlockCapabilityListener",
      "net.danygames2014.nyalib.init.fluid.EntityCapabilityListener",
      "net.danygames2014.nyalib.init.fluid.ItemCapabilityListener"
    ],
    "stationapi:event_bus_client": [
      "net.danygames2014.nyalib.init.fluid.TextureListener"
    ],
    "stationapi:event_bus_server": [
    ]
  },
  "mixins": [
    "nyalib-fluid.mixins.json"
  ],
  "accessWidener": "nyalib-fluid.accesswidener",
  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_633": ["net/danygames2014/nyalib/screen/FluidScreenHandlerListener"],
      "net/minecraft/class_71": ["net/danygames2014/nyalib/screen/FluidScreenHandler"],
      "net/minecraft/class_504": ["net/danygames2014/nyalib/client/FluidInteractionManager"],
      "net/minecraft/class_220": ["net/danygames2014/nyalib/fluid/FluidBucket"]
    }
  }
}
