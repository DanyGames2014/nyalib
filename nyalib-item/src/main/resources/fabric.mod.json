  "id": "nyalib-item",
  "version": "${version}",

  "name": "NyaLib Item",
  "description": "A module providing an Item API",
  "authors": [
    "DanyGames2014"
  ],

  "license": "MIT",
  "icon": "assets/nyalib-item/icon.png",

